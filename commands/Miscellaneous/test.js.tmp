const discord = require('discord.js'), Odesli = require('odesli.js');
module.exports = {
  name:'test',
  category:'Miscellaneous',
  description:'Test command',
  async execute(client, message, args) {
    const songlink = new Odesli({
      version:'v1-alpha.1'
    });
    let query = args[0]
    if (!args[0]) return message.channel.send('no url')
    let song = await songlink.fetch(query)
    if (!song.linksByPlatform.youtube.url) return message.channel.send('no song found')
    message.channel.send(song.linksByPlatform.youtube.url)
  }
}